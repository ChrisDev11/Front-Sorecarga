Ã§Ã£o Category\r\neditar(Categorys) {\r\n      document.getElementById(\"name\").value = Categorys.name\r\n      this.Categorys = Categorys\r\n    },\r\n    //Atualizar dados que ja estao gravados\r\n    alterar() {\r\n      this.Categorys.name = document.getElementById(\"name\").value \r\n      \r\n      axios.put(`https://localhost:3000/Category/${this.Categorys.id}`, this.Categorys).then(()=>{\r\n        this.lista()\r\n        this.Categorys = undefined\r\n        document.getElementById(\"name\").value = \"\"\r\n      })\r\n    },\r\n    //Metodo EXCLUIR \r\n    excluir(id) {\r\n      if(confirm(\"ðŸš«Deseja realmente excluir a Categoria?ðŸš«\")){\r\n      axios.delete(`https://localhost:3000/Category/${id}`).then(()=>{\r\n        this.lista()\r\n        })\r\n      }\r\n    }\r\n},\r\n  created(){\r\n    this.lista()\r\n  }\r\n\r\n  }\r\n</script>"],"mappings":"AA8CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,mBAAmB;EACzBC,KAAK,EAAE;IACPC,GAAG,EAAEC;EACP,CAAC;EACDC,IAAI,EAACA,CAAA,KAAM;IACX,OAAO;MACLC,QAAQ,EAAC,EAAE;MACTC,SAAS,EAAEC,SAAS;MACpBC,QAAQ,EAAE;IAEZ;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,KAAIA,CAAA,EAAK;MACPX,KAAK,CAACY,GAAG,CAAC,iCAAiC,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAG;QAExD,IAAI,CAACR,QAAO,GAAIQ,GAAG,CAACT,IAAG;MACxB,CAAC;IACH,CAAC;IACDU,MAAKA,CAAA,EAAK;MACR,IAAG,IAAI,CAACR,SAAS,EAAC;QAChB,IAAI,CAACS,OAAO,CAAC;QACb;MACF;MACAhB,KAAK,CAACiB,IAAI,CAAC,iCAAiC,EAC5C;QACEhB,IAAI,EAACiB,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC;MACvC,CAAC,CAAC,CAACP,IAAI,CAAC,MAAI;QACV,IAAI,CAACF,KAAK,CAAC;QACXU,OAAO,CAACC,GAAG,CAACR,GAAG;MACjB,CAAC;IACH,CAAC;IACL;IACAS,MAAMA,CAAChB,SAAS,EAAE;MACZW,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAI,GAAIb,SAAS,CAACN,IAAG;MACrD,IAAI,CAACM,SAAQ,GAAIA,SAAQ;IAC3B,CAAC;IACD;IACAS,OAAOA,CAAA,EAAG;MACR,IAAI,CAACT,SAAS,CAACN,IAAG,GAAIiB,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAI;MAE1DpB,KAAK,CAACwB,GAAG,CAAE,mCAAkC,IAAI,CAACjB,SAAS,CAACkB,EAAG,EAAC,EAAE,IAAI,CAAClB,SAAS,CAAC,CAACM,IAAI,CAAC,MAAI;QACzF,IAAI,CAACF,KAAK,CAAC;QACX,IAAI,CAACJ,SAAQ,GAAIC,