{"ast":null,"code":"import SideBarDash from \"@/components/SideBarDash.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"Neworder\",\n  components: {\n    SideBarDash\n  },\n  data() {\n    return {\n      purchases: []\n    };\n  },\n  created() {\n    this.fetchPurchaseData();\n  },\n  methods: {\n    fetchPurchaseData() {\n      // Faça uma solicitação GET usando axios\n      axios.get('https://localhost:3000/Purchase').then(response => {\n        // Atualize os dados de compra com os dados da resposta\n        this.purchases = response.data.map(purchase => ({\n          id: purchase.id,\n          buyDate: purchase.buyDate,\n          payment: purchase.payment,\n          status: purchase.status,\n          userId: purchase.userId\n        }));\n      }).catch(error => {\n        console.error('Erro ao buscar dados de compra:', error);\n      });\n    }\n    // viewDetails(purchaseId) {\n\n    // this.$router.push(`/detalhes-compra/${purchaseId}`);\n    // },\n  }\n};","map":{"version":3,"names":["SideBarDash","axios","name","components","data","purchases","created","fetchPurchaseData","methods","get","then","response","map","purchase","id","buyDate","payment","status","userId","catch","error","console"],"sources":["C:\\Users\\11\\Desktop\\frontend\\src\\views\\PainelAdm\\Neworder.vue"],"sourcesContent":["<template>\r\n  \r\n    <div class=\"d-flex\">\r\n\r\n<SideBarDash class=\"flex-shrink-0\" />\r\n<div class=\"container\">\r\n<h1>Painel de Acompanhamento de Vendas</h1>\r\n\r\n    <!-- Tabela de Vendas -->\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Número da Venda</th>\r\n          <th>Nome do Cliente</th>\r\n          <th>Data da Compra</th>\r\n          <th>Processo</th>\r\n          <th>Confirmar Pagamento</th>\r\n          <th>Valor Total</th>\r\n          <th>Detalhes</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"purchase in purchases\" :key=\"purchase.id\">\r\n          <td>{{ purchase.id }}</td>\r\n          <td>{{ purchase.userId }}</td>\r\n          <td>{{ purchase.buyDate }}</td>\r\n          <td>{{ purchase.status }}</td>\r\n          <td>\r\n            <button>Confirmar</button>\r\n          </td>\r\n          <!-- <td>{{ purchase.payment }}</td> -->\r\n          \r\n          <td>\r\n            <button @click=\"viewDetails(purchase.id)\" class=\"btn btn-primary\">Detalhes</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n</div>\r\n</div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport SideBarDash from \"@/components/SideBarDash.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Neworder\",\r\n  components: {\r\n    SideBarDash,\r\n  },\r\n  data(){\r\n    return{\r\n      purchases: [],\r\n    };\r\n  },\r\n\r\n  created(){\r\n    this.fetchPurchaseData();\r\n\r\n  },\r\n  methods:{\r\n    fetchPurchaseData() {\r\n      // Faça uma solicitação GET usando axios\r\n      axios.get('https://localhost:3000/Purchase')\r\n        .then((response) => {\r\n          // Atualize os dados de compra com os dados da resposta\r\n          this.purchases = response.data.map((purchase) => ({\r\n            id: purchase.id,\r\n            buyDate: purchase.buyDate,\r\n            payment: purchase.payment,\r\n            status: purchase.status,\r\n            userId: purchase.userId,\r\n          }));\r\n        })\r\n        .catch((error) => {\r\n          console.error('Erro ao buscar dados de compra:', error);\r\n        });\r\n    },\r\n    // viewDetails(purchaseId) {\r\n\r\n    // this.$router.push(`/detalhes-compra/${purchaseId}`);\r\n    // },\r\n  },\r\n \r\n\r\n\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"mappings":"AA8CA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,iBAAiB,CAAC