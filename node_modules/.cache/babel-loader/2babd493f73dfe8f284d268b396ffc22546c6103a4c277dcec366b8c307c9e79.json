{"ast":null,"code":"import axios from 'axios';\nimport jwtDecode from 'jwt-decode';\nimport Cookies from 'js-cookie';\nexport default {\n  name: \"accont\",\n  data() {\n    return {\n      endereco: \"https://rechargeapi.azurewebsites.net/\",\n      userId: null,\n      formData: {\n        nome: \"\",\n        cpf: \"\",\n        telefone: \"\",\n        email: \"\"\n      }\n    };\n  },\n  created() {\n    const jwtCookie = this.getCookie('authToken');\n\n    // Decode JWT token\n    if (jwtCookie) {\n      try {\n        const decodedToken = jwtDecode(jwtCookie);\n        if (decodedToken) {\n          this.jwtDecoded = decodedToken;\n          // Acesse o campo correto do JWT para obter o ID do usuário\n          const userId = decodedToken.Id; // Substitua 'sub' pelo campo correto em seu JWT\n          console.log(\"Token decodificado:\", decodedToken);\n          // Faça uma solicitação Axios para buscar os dados do usuário\n          axios.get(`${this.endereco}/Users/${userId}`).then(response => {\n            // Preencha os dados do usuário no formulário com base na resposta\n            this.cliente = response.data;\n          }).catch(error => {\n            console.error(\"Erro ao buscar os dados do usuário:\", error);\n          });\n        }\n      } catch (error) {\n        console.error(\"Erro ao decodificar o JWT:\", error);\n      }\n    }\n  },\n  methods: {\n    async fetchUserData() {\n      try {\n        const response = await axios.get(`${this.endereco}/Users/${this.userId}`);\n        // Manipule os dados do usuário aqui e preencha o formulário\n      } catch (error) {\n        console.error('Erro ao buscar dados do usuário:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","jwtDecode","Cookies","name","data","endereco","userId","formData","nome","cpf","telefone","email","created","jwtCookie","getCookie","decodedToken","jwtDecoded","Id","console","log","get","then","response","cliente","catch","error","methods","fetchUserData"],"sources":["C:\\Users\\11\\Desktop\\frontend\\src\\components\\Accont.vue"],"sourcesContent":["  <template>\r\n  <div>\r\n    <h2>Sua Conta</h2>\r\n    <form>\r\n      <div class=\"form-group\">\r\n        <label for=\"nome\">Nome:</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"nome\"\r\n          v-model=\"formData.nome\"\r\n          class=\"form-control\"\r\n          placeholder=\"Seu Nome\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"cpf\">CPF:</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"cpf\"\r\n          v-model=\"formData.cpf\"\r\n          class=\"form-control\"\r\n          placeholder=\"Seu CPF\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"telefone\">Telefone:</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"telefone\"\r\n          v-model=\"formData.telefone\"\r\n          class=\"form-control\"\r\n          placeholder=\"Seu Telefone\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email:</label>\r\n        <input\r\n          type=\"email\"\r\n          id=\"email\"\r\n          v-model=\"formData.email\"\r\n          class=\"form-control\"\r\n          placeholder=\"Seu Email\"\r\n        />\r\n      </div>\r\n\r\n      <button type=\"submit\" class=\"btn btn-primary\">Salvar</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport jwtDecode from 'jwt-decode';\r\nimport Cookies from 'js-cookie';\r\n\r\n\r\n    \r\nexport default {\r\n    name: \"accont\",\r\n    data(){\r\n        return{\r\n            endereco:\"https://rechargeapi.azurewebsites.net/\",\r\n            userId: null,\r\n\r\n\r\n        formData: {\r\n        nome: \"\",\r\n        cpf: \"\",\r\n        telefone: \"\",\r\n        email: \"\"\r\n      }\r\n        }\r\n    },\r\n    created() {\r\n        \r\n        const jwtCookie = this.getCookie('authToken');\r\n\r\n// Decode JWT token\r\n    if (jwtCookie) {\r\n      try {\r\n        const decodedToken = jwtDecode(jwtCookie);\r\n\r\n        if (decodedToken) {\r\n          this.jwtDecoded = decodedToken;\r\n          // Acesse o campo correto do JWT para obter o ID do usuário\r\n          const userId = decodedToken.Id; // Substitua 'sub' pelo campo correto em seu JWT\r\n          console.log(\"Token decodificado:\", decodedToken);\r\n          // Faça uma solicitação Axios para buscar os dados do usuário\r\n          axios\r\n            .get(`${this.endereco}/Users/${userId}`)\r\n            .then((response) => {\r\n              // Preencha os dados do usuário no formulário com base na resposta\r\n              this.cliente = response.data;\r\n            })\r\n            .catch((error) => {\r\n              console.error(\"Erro ao buscar os dados do usuário:\", error);\r\n            });\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Erro ao decodificar o JWT:\", error);\r\n      }\r\n    }\r\n},\r\n  methods: {\r\n    async fetchUserData() {\r\n      try {\r\n        const response = await axios.get(`${this.endereco}/Users/${this.userId}`);\r\n        // Manipule os dados do usuário aqui e preencha o formulário\r\n      } catch (error) {\r\n        console.error('Erro ao buscar dados do usuário:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAuDA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,SAAQ,MAAO,YAAY;AAClC,OAAOC,OAAM,MAAO,WAAW;AAI/B,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,QAAQ,EAAC,wCAAwC;MACjDC,MAAM,EAAE,IAAI;MAGhBC,QAAQ,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,GAAG,EAAE,EAAE;QACPC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE;MACT;IACE;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IAEN,MAAMC,SAAQ,GAAI,IAAI,CAACC,SAAS,CAAC,WAAW,CAAC;;IAErD;IACI,IAAID,SAAS,EAAE;MACb,IAAI;QACF,MAAME,YAAW,GAAId,SAAS,CAACY,SAAS,CAAC;QAEzC,IAAIE,YAAY,EAAE;UAChB,IAAI