ry\" v-on:click=\"back\">Cancelar</button>\r\n        <button type=\"submit\" class=\"btn btn-primary\" v-on:click=\"signup\">Criar</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\"\r\nimport Swal from 'sweetalert2'\r\n\r\n\r\nexport default ({\r\n    data(){\r\n        return {\r\n            user:{\r\n                result: 0,\r\n                Name:\"\",\r\n                Email:\"\",\r\n                Password:\"\",\r\n                ConfirmPassword:\"\"\r\n            },\r\n        }\r\n    },\r\n    methods:{\r\n        back(){\r\n            this.$router.push({ name: 'Login'});\r\n            \r\n        },\r\n        signup(){\r\n            \r\n            if (this.checkValidation()) {\r\n    axios\r\n      .post(\"https://localhost:3000/User/Register\", this.user)\r\n      .then((response) => {\r\n        const message = response.data.Message;\r\n        if (message) {\r\n          Swal.fire(message).then(() => {\r\n            this.back();\r\n          });\r\n        } else {\r\n          Swal.fire(\"Erro: Something went wrong\");\r\n        }\r\n      })\r\n      .catch((error) => {\r\n        if (error.response) {\r\n          Swal.fire(error.response.data);\r\n        }\r\n      });\r\n  }\r\n             //console.log(this.checkValidation());\r\n        },\r\n\r\n\r\n        checkValidation(){\r\n            if(!this.user.Name){\r\n                this.$refs.Name.focus();\r\n                Swal.fire(\"Give username !\");\r\n                return;\r\n            }\r\n            if(!this.user.Email){\r\n                this.$refs.Email.focus();\r\n                Swal.fire(\"Give Email !\");\r\n                return;\r\n            }\r\n            // if(!(/\\w+([-+.']\\w+)*@\\w+([-.]\\w+)*\\.w+([-.]\\w+)*/).test(this.user.Email)){\r\n            //     this.$refs.Email.focus();\r\n            //     Swal.fire(\"Invalid email !\");\r\n            //     return;\r\n     