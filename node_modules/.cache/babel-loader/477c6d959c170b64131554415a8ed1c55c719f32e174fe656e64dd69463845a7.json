{"ast":null,"code":"import SideBarDash from \"@/components/SideBarDash.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"CadastroProduto\",\n  components: {\n    SideBarDash\n  },\n  props: {\n    msg: String\n  },\n  data: () => {\n    return {\n      Product: [],\n      Products: undefined,\n      Product: \"\"\n    };\n  },\n  methods: {\n    //Lista os Produtos na tela\n    lista() {\n      axios.get(\"https://localhost:3000/Product\").then(res => {\n        this.Product = res.data;\n        // console.log(res);\n      });\n    },\n\n    salvar() {\n      if (this.Products) {\n        this.alterar();\n        return;\n      }\n      console.log(res);\n      axios.post(\"https://localhost:3000/Product\", {\n        sku: document.getElementById(\"sku\").value,\n        name: document.getElementById(\"name\").value,\n        brand: document.getElementById(\"brand\").value,\n        description: document.getElementById(\"description\").value,\n        price: document.getElementById(\"price\").value\n      }).then(() => {\n        this.lista();\n      });\n    },\n    //Auto preencher dados para a edi√ß√£o Products\n    editar(Products) {\n      document.getElementById(\"sku\").value = Products.sku;\n      document.getElementById(\"name\").value = Products.name;\n      document.getElementById(\"brand\").value = Products.brand;\n      document.getElementById(\"description\").value = Products.description;\n      document.getElementById(\"price\").value = Products.price;\n      this.Products = Products;\n    },\n    //Atualizar dados que ja estao gravados\n    alterar() {\n      this.Products.sku = document.getElementById(\"sku\").value;\n      this.Products.name = document.getElementById(\"name\").value;\n      this.Products.brand = document.getElementById(\"brand\").value;\n      this.Products.description = document.getElementById(\"description\").value;\n      this.Products.price = document.getElementById(\"price\").value;\n      axios.put(`https://localhost:3000/Product/${this.Products.id}`, this.Products).then(() => {\n        this.lista();\n        this.Products = undefined;\n        document.getElementById(\"sku\").value = \"\";\n        document.getElementById(\"name\").value = \"\";\n        document.getElementById(\"brand\").value = \"\";\n        document.getElementById(\"description\").value = \"\";\n        document.getElementById(\"price\").value = \"\";\n      });\n    },\n    //Metodo EXCLUIR\n    excluir(id) {\n      if (confirm(\"üö´Deseja realmente excluir o Produto?üö´\")) {\n        axios.delete(`https://localhost:3000/Product/${id}`).then(() => {\n          this.lista();\n        });\n      }\n    }\n  },\n  created() {\n    this.lista();\n  }\n};","map":{"version":3,"names":["SideBarDash","axios","name","components","props","msg","String","data","Product","Products","undefined","methods","lista","get","then","res","salvar","alterar","console","log","post","sku","document","getElementById","value","brand","description","price","editar","put","id","excluir","confirm","delete","created"],"sources":["C:\\Users\\11\\Desktop\\frontend\\src\\views\\PainelAdm\\CadastroProduto.vue"],"sourcesContent":["<template>\r\n  <div class=\"d-flex\">\r\n    <!-- Use a classe \"d-flex\" para criar um layout flex√≠vel -->\r\n    <SideBarDash class=\"flex-shrink-0\" />\r\n    <!-- Adicione a classe \"flex-shrink-0\" para evitar que a barra lateral cres√ßa -->\r\n    <div class=\"flex-grow-1\">\r\n      <!-- Use a classe \"flex-grow-1\" para que o conte√∫do ocupe o espa√ßo restante -->\r\n      <div class=\"container mt-3\">\r\n        <h3>Cadastro de Produtos</h3>\r\n\r\n        <form class=\"row g-3\">\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Sku</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"sku\"\r\n              name=\"sku\"\r\n              placeholder=\"Digite o Sku\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"col-md-4\">\r\n            <label class=\"form-label\">Produto</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"name\"\r\n              name=\"name\"\r\n              placeholder=\"Digite o nome do Produto\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Marca</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"brand\"\r\n              name=\"brand\"\r\n              placeholder=\"Marca\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Pre√ßo</label>\r\n            <input\r\n              type=\"number\"\r\n              class=\"form-control\"\r\n              id=\"price\"\r\n              name=\"price\"\r\n              placeholder=\"Digite o Valor do Produto\"\r\n            />\r\n          </div>\r\n\r\n          <!-- <div class=\"col-md-2\">\r\n     <label  class=\"form-label\">Quantidade</label>\r\n      <input type=\"number\" class=\"form-control\" id=\"stock\" name=\"stock\" placeholder=\"Qtd\" aria-label=\"Qtd\">\r\n    </div> -->\r\n    \r\n\r\n          <div class=\"col-12\">\r\n            <label class=\"form-label\">Descri√ß√£o</label>\r\n            <!-- <input type=\"text\" class=\"form-control\" id=\"description\" name=\"description\" placeholder=\"Descri√ß√£o do Produto\"> -->\r\n            <textarea\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"description\"\r\n              name=\"description\"\r\n              placeholder=\"Descri√ß√£o do Produto\"\r\n            ></textarea>\r\n          </div>\r\n\r\n          <div class=\"col-12\">\r\n            <button v-on:click=\"salvar()\" type=\"submit\" class=\"btn btn-primary\">\r\n              Salvar\r\n            </button>\r\n          </div>\r\n        </form>\r\n        <hr />\r\n\r\n        <table class=\"table\">\r\n          <thead>\r\n            <tr>\r\n              <th>Sku</th>\r\n              <th>Produto</th>\r\n              <th>Pre√ßo</th>\r\n              <th>Descri√ß√£o</th>\r\n              <th>Marca</th>\r\n              <!-- <th colspan=\"2\"></th> -->\r\n            </tr>\r\n          </thead>\r\n\r\n          <tbody>\r\n            <tr v-for=\"Products in Product\" v-bind:key=\"Products.id\">\r\n              <td>\r\n                {{ Products.sku }}\r\n              </td>\r\n              <td>\r\n                {{ Products.name }}\r\n              </td>\r\n              <td>\r\n                {{ Products.price }}\r\n              </td>\r\n              <td>\r\n                {{ Products.description }}\r\n              </td>\r\n              <td>\r\n                {{ Products.brand }}\r\n              </td>\r\n              <td>\r\n                <button class=\"btn btn-primary\" v-on:click=\"editar(Products)\">\r\n                  Editar\r\n                </button>\r\n              </td>\r\n              <td>\r\n                <button\r\n                  class=\"btn btn-danger\"\r\n                  v-on:click=\"excluir(Products.id)\"\r\n                >\r\n                  Excluir\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SideBarDash from \"@/components/SideBarDash.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"CadastroProduto\",\r\n  components: {\r\n    SideBarDash,\r\n  },\r\n\r\n  props: {\r\n    msg: String,\r\n  },\r\n\r\n  data: () => {\r\n    return {\r\n      Product: [],\r\n      Products: undefined,\r\n      Product: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    //Lista os Produtos na tela\r\n    lista() {\r\n      axios.get(\"https://localhost:3000/Product\").then((res) => {\r\n        this.Product = res.data;\r\n        // console.log(res);\r\n      });\r\n    },\r\n    salvar() {\r\n      if (this.Products) {\r\n        this.alterar();\r\n        return;\r\n      }\r\n      console.log(res);\r\n      axios\r\n        .post(\"https://localhost:3000/Product\", {\r\n          sku: document.getElementById(\"sku\").value,\r\n          name: document.getElementById(\"name\").value,\r\n          brand: document.getElementById(\"brand\").value,\r\n          description: document.getElementById(\"description\").value,\r\n          price: document.getElementById(\"price\").value,\r\n        })\r\n        .then(() => {\r\n          this.lista();\r\n\r\n          \r\n        });\r\n    },\r\n    //Auto preencher dados para a edi√ß√£o Products\r\n    editar(Products) {\r\n      document.getElementById(\"sku\").value = Products.sku;\r\n      document.getElementById(\"name\").value = Products.name;\r\n      document.getElementById(\"brand\").value = Products.brand;\r\n      document.getElementById(\"description\").value = Products.description;\r\n      document.getElementById(\"price\").value = Products.price;\r\n      this.Products = Products;\r\n    },\r\n    //Atualizar dados que ja estao gravados\r\n    alterar() {\r\n      this.Products.sku = document.getElementById(\"sku\").value;\r\n      this.Products.name = document.getElementById(\"name\").value;\r\n      this.Products.brand = document.getElementById(\"brand\").value;\r\n      this.Products.description = document.getElementById(\"description\").value;\r\n      this.Products.price = document.getElementById(\"price\").value;\r\n\r\n      axios\r\n        .put(\r\n          `https://localhost:3000/Product/${this.Products.id}`,\r\n          this.Products\r\n        )\r\n        .then(() => {\r\n          this.lista();\r\n          this.Products = undefined;\r\n          document.getElementById(\"sku\").value = \"\";\r\n          document.getElementById(\"name\").value = \"\";\r\n          document.getElementById(\"brand\").value = \"\";\r\n          document.getElementById(\"description\").value = \"\";\r\n          document.getElementById(\"price\").value = \"\";\r\n        });\r\n    },\r\n    //Metodo EXCLUIR\r\n    excluir(id) {\r\n      if (confirm(\"üö´Deseja realmente excluir o Produto?üö´\")) {\r\n        axios.delete(`https://localhost:3000/Product/${id}`).then(() => {\r\n          this.lista();\r\n        });\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.lista();\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":"AAoIA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVH;EACF,CAAC;EAEDI,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EAEDC,IAAI,EAAEA,CAAA,KAAM;IACV,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAEC,SAAS;MACnBF,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACP;IACAC,KAAKA,CAAA,EAAG;MACNX,KAAK,CAACY,GAAG,CAAC,gCAAgC,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;QACxD,IAAI,CAACP,OAAM,GAAIO,GAAG,CAACR,IAAI;QACvB;MACF,CAAC,CAAC;IACJ,CAAC;;IACDS,MAAMA,CAAA,EAAG;MACP,IAAI,IAAI,CAACP,QAAQ,EAAE;QACjB,IAAI,CAACQ,OAAO,CAAC,CAAC;QACd;MACF;MACAC,OAAO,CAACC,GAAG,CAACJ,GAAG,CAAC;MAChBd,KAAI,CACDmB,IAAI,CAAC,gCAAgC,EAAE;QACtCC,GAAG,EAAEC,QAAQ,CAACC,cAAc,CAAC,KAAK,CAAC,CAACC,KAAK;QACzCtB,IAAI,EAAEoB,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAK;QAC3CC,KAAK,EAAEH,QAAQ,CAACC,cAAc,CAAC,OAAO,CAAC,CAACC,KAAK;QAC7CE,WAAW,EAAEJ,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAACC,KAAK;QACzDG,KAAK,EAAEL,QAAQ,CAACC,cAAc,CAAC,OAAO,CAAC,CAACC;MAC1C,CAAC,EACAV,IAAI,CAAC,MAAM;QACV,IAAI,CAACF,KAAK,CAAC,CAAC;MAGd,CAAC,CAAC;IACN,CAAC;IACD;IACAgB,MAAMA,CAACnB,QAAQ,EAAE;MACfa,QAAQ,CAACC,cAAc,CAAC,KAAK,CAAC,CAACC,KAAI,GAAIf,QAAQ,CAACY,GAAG;MACnDC,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAI,GAAIf,QAAQ,CAACP,IAAI;MACrDoB,QAAQ,CAACC,cAAc,CAAC,OAAO,CAAC,CAACC,KAAI,GAAIf,QAAQ,CAACgB,KAAK;MACvDH,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAACC,KAAI,GAAIf,QAAQ,CAACiB,WAAW;MACnEJ,QAAQ,CAACC,cAAc,CAAC,OAAO,CAAC,CAACC,KAAI,GAAIf,QA