{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport SlidePanel from \"@/components/SlidePanel.vue\";\nimport { ref } from \"vue\";\nimport Cookies from 'js-cookie';\nexport default {\n  components: {\n    SlidePanel\n  },\n  data() {\n    return {\n      products: [],\n      cart: [],\n      productsByBrand: {},\n      productsByCategory: {}\n    };\n  },\n  setup() {\n    const cartVisible = ref(false); // Variável de controle da visibilidade do carrinho\n\n    const changeCartVisibility = () => {\n      cartVisible.value = !cartVisible.value; // Função para alternar a visibilidade do carrinho\n      console.log(cartVisible.value);\n    };\n    return {\n      cartVisible,\n      changeCartVisibility\n    };\n  },\n  mounted() {\n    this.fetchProducts(); // Chama a função para buscar os produtos quando a página é carregada\n\n    // Recupere o carrinho do cookie quando a página é carregada\n    const savedCart = Cookies.get('cart'); // Corrija a chamada para Cookies.get\n    if (savedCart) {\n      this.cart = Array.isArray(savedCart) ? savedCart : JSON.parse(savedCart); // Converta o JSON do cookie de volta para um objeto\n    }\n  },\n\n  methods: {\n    // Adicionar produto ao carrinho\n    addToCart(product) {\n      const existingProduct = this.cart.find(item => item.id === product.id);\n      if (existingProduct) {\n        existingProduct.quantity++;\n      } else {\n        this.cart.push({\n          ...product,\n          quantity: 1\n        });\n      }\n\n      // Salve o carrinho em cookies com expiração de 1 hora\n      Cookies.set('cart', JSON.stringify(this.cart), {\n        expires: 1 / 24\n      }); // 1/24 representa 1 hora\n\n      console.log(this.cart); // Exibe o carrinho atualizado no console\n    },\n\n    fetchProducts() {\n      axios.get(\"https://localhost:3000/Product\").then(response => {\n        const products = response.data;\n\n        // Organize os produtos por marca e categoria\n        products.forEach(product => {\n          const brand = product.brand;\n          const category = product.category;\n          if (!this.productsByBrand[brand]) {\n            this.productsByBrand[brand] = [];\n          }\n          if (!this.productsByCategory[category]) {\n            this.productsByCategory[category] = [];\n          }\n          this.productsByBrand[brand].push(product);\n          this.productsByCategory[category].push(product);\n        });\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","SlidePanel","ref","Cookies","components","data","products","cart","productsByBrand","productsByCategory","setup","cartVisible","changeCartVisibility","value","console","log","mounted","fetchProducts","savedCart","get","Array","isArray","JSON","parse","methods","addToCart","product","existingProduct","find","item","id","quantity","push","set","stringify","expires","then","response","forEach","brand","category","catch","error"],"sources":["C:\\Users\\11\\Desktop\\frontend\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"banner\">\r\n      <!-- Conteúdo do banner -->\r\n      <h1>Bem-vindo à nossa loja!</h1>\r\n      <p>Encontre os melhores produtos aqui.</p>\r\n      <button class=\"btn btn-primary\" @click=\"changeCartVisibility()\">Exibir Carrinho</button>\r\n\r\n      <SlidePanel\r\n        title = \"Cart\"\r\n        :visible=\"cartVisible\"\r\n        @update:visible=\"changeCartVisibility\"\r\n        :cart=\"cart\"\r\n      ></SlidePanel>\r\n\r\n    </div>\r\n\r\n    <div class=\"container\">\r\n  <h2>Nossos Produtos por Marca</h2>\r\n  <div v-for=\"(products, brand) in productsByBrand\" :key=\"brand\">\r\n    <h3>{{ brand }}</h3>\r\n    <div class=\"product-list\">\r\n      <div v-for=\"product in products\" :key=\"product.id\" class=\"product-card\">\r\n        <!-- Conteúdo do cartão de produto -->\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <h2>Nossos Produtos por Categoria</h2>\r\n  <div v-for=\"(products, category) in productsByCategory\" :key=\"category\">\r\n    <h3>{{ category }}</h3>\r\n    <div class=\"product-list\">\r\n      <div v-for=\"product in products\" :key=\"product.id\" class=\"product-card\">\r\n        <!-- Conteúdo do cartão de produto -->\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport SlidePanel from \"@/components/SlidePanel.vue\"\r\nimport { ref } from \"vue\";\r\nimport Cookies from 'js-cookie';\r\n\r\nexport default {\r\n  components: {\r\n    SlidePanel\r\n  },\r\n  data() {\r\n    return {\r\n      products: [],\r\n      cart: [],\r\n      productsByBrand: {},\r\n    productsByCategory: {},\r\n    };\r\n  },\r\n  setup() {\r\n    const cartVisible = ref(false); // Variável de controle da visibilidade do carrinho\r\n\r\n    const changeCartVisibility = () => {\r\n      cartVisible.value = !cartVisible.value; // Função para alternar a visibilidade do carrinho\r\n      console.log(cartVisible.value);\r\n    };\r\n\r\n    return {\r\n      cartVisible,\r\n      changeCartVisibility,\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.fetchProducts(); // Chama a função para buscar os produtos quando a página é carregada\r\n\r\n    // Recupere o carrinho do cookie quando a página é carregada\r\n    const savedCart = Cookies.get('cart'); // Corrija a chamada para Cookies.get\r\n    if (savedCart) {\r\n      this.cart = Array.isArray(savedCart) ? savedCart : JSON.parse(savedCart); // Converta o JSON do cookie de volta para um objeto\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    // Adicionar produto ao carrinho\r\n    addToCart(product) {\r\n      const existingProduct = this.cart.find(item => item.id === product.id);\r\n\r\n      if (existingProduct) {\r\n        existingProduct.quantity++;\r\n      } else {\r\n        this.cart.push({ ...product, quantity: 1 });\r\n      }\r\n\r\n      // Salve o carrinho em cookies com expiração de 1 hora\r\n      Cookies.set('cart', JSON.stringify(this.cart), { expires: 1 / 24 }); // 1/24 representa 1 hora\r\n\r\n      console.log(this.cart); // Exibe o carrinho atualizado no console\r\n    },\r\n\r\n    fetchProducts() {\r\n      axios\r\n    .get(\"https://localhost:3000/Product\")\r\n    .then((response) => {\r\n      const products = response.data;\r\n\r\n      // Organize os produtos por marca e categoria\r\n      products.forEach((product) => {\r\n        const brand = product.brand;\r\n        const category = product.category;\r\n\r\n        if (!this.productsByBrand[brand]) {\r\n          this.productsByBrand[brand] = [];\r\n        }\r\n\r\n        if (!this.productsByCategory[category]) {\r\n          this.productsByCategory[category] = [];\r\n        }\r\n\r\n        this.productsByBrand[brand].push(product);\r\n        this.productsByCategory[category].push(product);\r\n      });\r\n    })\r\n    .catch((error) => {\r\n      console.log(error);\r\n    });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.banner {\r\n  background-color: #f5f5f5;\r\n  text-align: center;\r\n  padding: 20px;\r\n}\r\n\r\n.container {\r\n  margin-top: 20px;\r\n}\r\n\r\n.product-card {\r\n  text-decoration: none;\r\n  display: inline-block;\r\n  width: 300px;\r\n  margin: 10px;\r\n  padding: 10px;\r\n  border: 1px solid #ccc;\r\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\r\n  transition: transform 0.2s ease-in-out;\r\n  background-color: #fff;\r\n}\r\n\r\n.product-card:hover {\r\n  transform: translateY(-5px);\r\n}\r\n\r\n/* Estilo para a imagem do produto */\r\n.product-image {\r\n  max-width: 100%;\r\n  height: auto;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n/* Estilo para o nome do produto */\r\n.product-card h3 {\r\n  font-size: 18px;\r\n  margin-top: 10px;\r\n  text-align: center;\r\n}\r\n\r\n/* Estilo para a descrição do produto */\r\n.product-description {\r\n  font-size: 14px;\r\n  color: #555;\r\n  margin-top: 8px;\r\n  text-align: center;\r\n}\r\n\r\n/* Estilo para o preço do produto */\r\n.product-price {\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  color: #e67e22;\r\n  margin-top: 10px;\r\n  text-align: center;\r\n}\r\n\r\n/* Estilo para os botões de ação */\r\n.product-buttons {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.buy-button,\r\n.add-to-cart-button {\r\n  padding: 10px 15px;\r\n  font-size: 14px;\r\n  cursor: pointer;\r\n  border: none;\r\n  border-radius: 4px;\r\n  width: 120px;\r\n  text-align: center;\r\n}\r\n\r\n.buy-button {\r\n  background-color: #e67e22;\r\n  color: #fff;\r\n  margin-right: 10px;\r\n  transition: background-color 0.2s ease-in-out;\r\n}\r\n\r\n.buy-button:hover {\r\n  background-color: #d35400;\r\n}\r\n\r\n.add-to-cart-button {\r\n  background-color: #3498db;\r\n  color: #fff;\r\n  transition: background-color 0.2s ease-in-out;\r\n}\r\n\r\n.add-to-cart-button:hover {\r\n  background-color: #2980b9;\r\n}\r\n</style>\r\n"],"mappings":";AA0CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,UAAS,MAAO,6BAA4B;AACnD,SAASC,GAAE,QAAS,KAAK;AACzB,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACbC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,eAAe,EAAE,CAAC,CAAC;MACrBC,kBAAkB,EAAE,CAAC;IACrB,CAAC;EACH,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,WAAU,GAAIT,GAAG,CAAC,KAAK,CAAC,EAAE;;IAEhC,MAAMU,oBAAmB,GAAIA,CAAA,KAAM;MACjCD,WAAW,CAACE,KAAI,GAAI,CAACF,WAAW,CAACE,KAAK,EAAE;MACxCC,OAAO,CAACC,GAAG,CAACJ,WAAW,CAACE,KAAK,CAAC;IAChC,CAAC;IAED,OAAO;MACLF,WAAW;MACXC;IACF,CAAC;EACH,CAAC;EAEDI,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC,EAAE;;IAEtB;IACA,MAAMC,SAAQ,GAAIf,OAAO,CAACgB,GAAG,CAAC,MAAM,CAAC,EAAE;IACvC,IAAID,SAAS,EAAE;MACb,IAAI,CAACX,IAAG,GAAIa,KAAK,CAACC,OAAO,CAACH,SAAS,IAAIA,SAAQ,GAAII,IAAI,CAACC,KAAK,CAACL,SAAS,CAAC,EAAE;IAC5E;EACF,CAAC;;EAEDM,OAAO,EAAE;IACP;IACAC,SAASA,CAACC,OAAO,EAAE;MACjB,MAAMC,eAAc,GAAI,IAAI,CAACpB,IAAI,CAACqB,IAAI,CAACC,IAAG