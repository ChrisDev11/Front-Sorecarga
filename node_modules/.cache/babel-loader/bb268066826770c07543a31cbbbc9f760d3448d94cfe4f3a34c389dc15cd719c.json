ustify-content-lg-center\">\r\n          <!-- Link para a página Home -->\r\n          <li class=\"nav-item\" style=\"background-color: red;\">\r\n            <router-link class=\"nav-link active\" aria-current=\"page\" to=\"/\">Home</router-link>\r\n          </li>\r\n\r\n          <!-- Link para a página de Produtos -->\r\n          <li class=\"nav-item\">\r\n            <router-link class=\"nav-link\" to=\"/Produtos\">Produtos</router-link>\r\n          </li>\r\n\r\n          <!-- Item de Navegação Desativado (Em breve) -->\r\n          <li class=\"nav-item\">\r\n            <a class=\"nav-link disabled\">Em breve</a>\r\n          </li>\r\n\r\n          <!-- Dropdown de Cadastros -->\r\n          <li class=\"nav-item dropdown\">\r\n            <a class=\"nav-link dropdown-toggle\" href=\"#\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">Cadastros</a>\r\n            <ul class=\"dropdown-menu\">\r\n              <!-- Links para Páginas de Cadastro -->\r\n              <li><router-link class=\"dropdown-item\" to=\"/CadastroCategoria\">Cadastro de Categorias</router-link></li>\r\n              <li><router-link class=\"dropdown-item\" to=\"/CadastroMarca\">Cadastro de Marca</router-link></li>\r\n              <li><router-link class=\"dropdown-item\" to=\"/CadastroProduto\">Cadastro de Produtos</router-link></li>\r\n              <li><router-link class=\"dropdown-item disabled\" to=\"#\">Cadastro de Ficha Técnica</router-link></li>\r\n            </ul>\r\n          </li>\r\n        </ul>\r\n\r\n        <!-- Botão de Login / Logout -->\r\n        <div class=\"d-lg-flex col-lg-3 justify-content-lg-end\">\r\n          <!-- Verifica se o usuário está logado -->\r\n          <template v-if=\"!loggedIn\">\r\n            <!-- Link para a página de Login -->\r\n            <router-link to=\"/Login\" class=\"nav-link\" style=\"font-weight: bold; color: white;\">Entrar / Cadastrar</router-link>\r\n          </template>\r\n          <template v-else>\r\n            <!-- Botão para efetuar o logout -->\r\n            <a class=\"nav-link\" @click=\"logout\" style=\"font-weight: bold; color: white;\">Sair</a>\r\n          </template>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </nav>\r\n</template>\r\n\r\n<script>\r\nimport { ref, watchEffect } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport Cookies from 'js-cookie';\r\n\r\nexport default {\r\n  name: 'Navbar',\r\n  setup() {\r\n    const loggedIn = ref(false); // Supondo que o usuário não está logado inicialmente\r\n    const router = useRouter();\r\n\r\n    // Verifica se o usuário está logado\r\n    watchEffect( () => {\r\n      if (Cookies.get('authToken')) {\r\n      console.log(Cookies.get('authToken'));\r\n      loggedIn.value = true;\r\n    } else{\r\n      \r\n    }\r\n    })\r\n\r\n    // Função para efetuar o logout\r\n    const logout = () => {\r\n      // Lógica para efetuar o logout (por exemplo, limpar o token e redirecionar)\r\n      Cookies.remove('authToken');\r\n      loggedIn.value = false;\r\n      router.push('/login'); // Redireciona para a página de login após o logout\r\n    };\r\n\r\n    return {\r\n      loggedIn,\r\n      logout,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.navbar {\r\n  background-color: #333;\r\n  box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n\r\n.navbar-brand {\r\n  font-weight: bold;\r\n  font-size: 1.2rem;\r\n  color: #fff;\r\n}\r\n\r\n\r\n.nav-link,\r\n.dropdown-toggle {\r\n  color: #fff;\r\n}\r\n\r\n.nav-link.disabled {\r\n  color: #6c757d;\r\n}\r\n\r\n.dropdown-item {\r\n  color: #212529;\r\n}\r\n.nav-item{\r\n  background: #F3F9FB;\r\n  border-radius: 18px;\r\n  font-family: Poppins;\r\n  font-size: 14px;\r\n  font-style: italic;\r\n  font-weight: 700;\r\n  line-height: 18px;\r\n  letter-spacing: 0em;\r\n  text-align: right;\r\n  margin-right: 30px;\r\n  display: flex;\r\n}\r\n\r\n\r\n</style>"],"mappings":";AA6DA,SAASA,GAAG,EAAEC,WAAU,QAAS,KAAK;AACtC,SAASC,SAAQ,QAAS,YAAY;AACtC,OAAOC