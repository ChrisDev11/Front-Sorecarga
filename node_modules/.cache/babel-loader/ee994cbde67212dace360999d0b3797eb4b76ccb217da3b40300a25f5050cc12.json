d(\"name\").value,\r\n        })\r\n        .then(() => {\r\n          this.lista();\r\n          console.log(res);\r\n        });\r\n    },\r\n    //Auto preencher dados para a ediÃ§Ã£o Brand\r\n    editar(Brand) {\r\n      document.getElementById(\"name\").value = Brand.name;\r\n      this.Brand = Brand;\r\n    },\r\n    //Atualizar dados que ja estao gravados\r\n    alterar() {\r\n      this.Brand.name = document.getElementById(\"name\").value;\r\n\r\n      axios\r\n        .put(`https://localhost:3000/Brand/${this.Brand.id}`, this.Brand)\r\n        .then(() => {\r\n          this.lista();\r\n          this.Brand = undefined;\r\n          document.getElementById(\"name\").value = \"\";\r\n        });\r\n    },\r\n    //Metodo EXCLUIR\r\n    excluir(id) {\r\n      if (confirm(\"ðŸš«Deseja realmente excluir a Marca?ðŸš«\")) {\r\n        axios.delete(`https://localhost:3000/brand/${id}`).then(() => {\r\n            console.log(this.excluir)\r\n          this.lista();\r\n        });\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.lista();\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAsDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EACDC,IAAI,EAAEA,CAAA,KAAM;IACV,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAEC,SAAS;MAChBF,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACNV,KAAK,CAACW,GAAG,CAAC,8BAA8B,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;QACtD,IAAI,CAACP,MAAK,GAAIO,GAAG,CAACR,IAAI;QACtBS,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB,CAAC,CAAC;IACJ,CAAC;IACDG,MAAMA,CAAA,EAAG;MACP,IAAI,IAAI,CAACT,KAAK,EAAE;QACd,IAAI,CAACU,OAAO,CAAC,CAAC;QACd;MACF;MACAjB,KAAI,CACDkB,IAAI,CAAC,8BAA8B,EAAE;QACpCjB,IAAI,EAAEkB,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC;MACxC,CAAC,EACAT,IAAI,CAAC,MAAM;QACV,IAAI,CAACF,KAAK,CAAC,CAAC;QACZI,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB,CAAC,CAAC;IACN,CAAC;IACD;IACAS,MAAMA,CAACf,KAAK,EAAE;MACZY,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAI,GAAId,KAAK,CAACN,IAAI;MAClD,IAAI,CAACM,KAAI,GAAIA,KAAK;IAC