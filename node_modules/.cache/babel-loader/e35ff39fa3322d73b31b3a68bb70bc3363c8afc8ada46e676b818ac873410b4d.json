oDisplayString(Brands.name), 9 /* TEXT, PROPS */, _hoisted_19);\n  }), 128 /* KEYED_FRAGMENT */))], 512 /* NEED_PATCH */), [[_vModelSelect, _ctx.selectedBrand]])]), _createCommentVNode(\" ????????? \"), _hoisted_20, _createElementVNode(\"div\", _hoisted_21, [_createElementVNode(\"button\", {\n    onClick: _cache[2] || (_cache[2] = $event => $options.salvar()),\n    type: \"submit\",\n    class: \"btn btn-primary\"\n  }, \" Salvar \")])]), _hoisted_22, _createElementVNode(\"table\", _hoisted_23, [_hoisted_24, _createElementVNode(\"tbody\", null, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList(_ctx.Product, Products => {\n    return _openBlock(), _createElementBlock(\"tr\", {\n      key: Products.id\n    }, [_createElementVNode(\"td\", null, _toDisplayString(Products.sku), 1 /* TEXT */), _createElementVNode(\"td\", null, _toDisplayString(Products.name), 1 /* TEXT */), _createElementVNode(\"td\", null, _toDisplayString(Products.price), 1 /* TEXT */), _createElementVNode(\"td\", null, _toDisplayString(Products.amount), 1 /* TEXT */), _createElementVNode(\"td\", null, _toDisplayString(Products.description), 1 /* TEXT */), _createElementVNode(\"td\", null, _toDisplayString(_ctx.brandNames[Products.brandId] || \"N/A\"), 1 /* TEXT */), _createElementVNode(\"td\", null, _toDisplayString(_ctx.categoryNames[Products.categoryId] || \"N/A\"), 1 /* TEXT */), _createElementVNode(\"td\", null, _toDisplayString(Products.barCode), 1 /* TEXT */), _createElementVNode(\"td\", null, [_createElementVNode(\"button\", {\n      class: \"btn btn-primary\",\n      onClick: $event => $options.editar(Products)\n    }, \" Editar \", 8 /* PROPS */, _hoisted_25)]), _createElementVNode(\"td\", null, [_createElementVNode(\"button\", {\n      class: \"btn btn-danger\",\n      onClick: $event => $options.excluir(Products.id)\n    }, \" Excluir \", 8 /* PROPS */, _hoisted_26)])]);\n  }), 128 /* KEYED_FRAGMENT */))])])])])]);\n}","map":{"version":3,"names":["class","_createElementVNode","type","ref","id","name","placeholder","_createCommentVNode","_createElementBlock","_hoisted_1","_createVNode","_component_SideBarDash","_hoisted_2","_hoisted_3","_hoisted_4","_hoisted_5","_hoisted_6","_hoisted_11","_hoisted_12","_hoisted_13","_hoisted_14","_hoisted_15","_ctx","selectedCategory","$event","_Fragment","_renderList","Category","Categorys","value","key","Id","_hoisted_16","_hoisted_17","_hoisted_18","selectedBrand","Brand","Brands","_hoisted_19","_hoisted_20","_hoisted_21","onClick","_cache","$options","salvar","_hoisted_22","_hoisted_23","_hoisted_24","Product","Products","_toDisplayString","sku","price","amount","description","brandNames","brandId","categoryNames","categoryId","barCode","editar","_hoisted_25","excluir","_hoisted_26"],"sources":["C:\\Users\\11\\Desktop\\frontend\\src\\views\\PainelAdm\\CadastroProduto.vue"],"sourcesContent":["<template>\r\n  <div class=\"d-flex\">\r\n    <!-- Use a classe \"d-flex\" para criar um layout flexível -->\r\n    <SideBarDash class=\"flex-shrink-0\" />\r\n    <!-- Adicione a classe \"flex-shrink-0\" para evitar que a barra lateral cresça -->\r\n    <div class=\"flex-grow-1\">\r\n      <!-- Use a classe \"flex-grow-1\" para que o conteúdo ocupe o espaço restante -->\r\n      <div class=\"container mt-3\">\r\n        <h3>Cadastro de Produtos</h3>\r\n\r\n        <form class=\"row g-3\">\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Sku</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"sku\"\r\n              name=\"sku\"\r\n              placeholder=\"Digite o Sku\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"col-md-4\">\r\n            <label class=\"form-label\">Produto</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"name\"\r\n              name=\"name\"\r\n              placeholder=\"Digite o nome do Produto\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Preço</label>\r\n            <input\r\n              type=\"number\"\r\n              class=\"form-control\"\r\n              id=\"price\"\r\n              name=\"price\"\r\n              placeholder=\"Digite o Valor do Produto\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Quantidade</label>\r\n            <input\r\n              type=\"number\"\r\n              class=\"form-control\"\r\n              id=\"amount\"\r\n              name=\"amount\"\r\n              placeholder=\"Qtd\"\r\n              aria-label=\"Qtd\"\r\n            />\r\n          </div>\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Código de Barras</label>\r\n            <input\r\n              type=\"number\"\r\n              class=\"form-control\"\r\n              id=\"barCode\"\r\n              name=\"barCode\"\r\n              placeholder=\"Código de Barras\"\r\n              aria-label=\"Código de Barras\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"col-md-4\">\r\n  <label class=\"form-label\">Imagem do Produto</label>\r\n  <input type=\"file\" ref=\"imageFile\" class=\"form-control-file\" />\r\n</div>\r\n          <!-- Recente Dropdowns -->\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Categoria</label>\r\n            <select v-model=\"selectedCategory\" class=\"form-select\">\r\n              <option\r\n                v-for=\"Categorys in Category\"\r\n                :value=\"Categorys.id\"\r\n                :key=\"Categorys.Id\"\r\n              >\r\n                {{ Categorys.name }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"col-md-2\">\r\n            <label class=\"form-label\">Marca</label>\r\n            <select v-model=\"selectedBrand\" class=\"form-select\">\r\n              <option\r\n                v-for=\"Brands in Brand\"\r\n                :value=\"Brands.id\"\r\n                :key=\"Brands.id\"\r\n              >\r\n                {{ Brands.name }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n          <!-- ????????? -->\r\n\r\n          <div class=\"col-12\">\r\n            <label class=\"form-label\">Descrição</label>\r\n            <textarea\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"description\"\r\n              name=\"description\"\r\n              placeholder=\"Descrição do Produto\"\r\n            ></textarea>\r\n          </div>\r\n\r\n          <div class=\"col-12\">\r\n            <button v-on:click=\"salvar()\" type=\"submit\" class=\"btn btn-primary\">\r\n              Salvar\r\n            </button>\r\n          </div>\r\n        </form>\r\n        <hr />\r\n\r\n        <table class=\"table\">\r\n          <thead>\r\n            <tr>\r\n              <th>Sku</th>\r\n              <th>Produto</th>\r\n              <th>Preço</th>\r\n              <th>Quantidade</th>\r\n              <th>Descrição</th>\r\n              <th>Marca</th>\r\n              <th>Categoria</th>\r\n              <th>Códico de barras</th>\r\n              <!-- <th colspan=\"2\"></th> -->\r\n            </tr>\r\n          </thead>\r\n\r\n          <tbody>\r\n            <tr v-for=\"Products in Product\" :key=\"Products.id\">\r\n              <td>\r\n                {{ Products.sku }}\r\n              </td>\r\n              <td>\r\n                {{ Products.name }}\r\n              </td>\r\n              <td>\r\n                {{ Products.price }}\r\n              </td>\r\n              <td>\r\n                {{ Products.amount }}\r\n              </td>\r\n              <td>\r\n                {{ Products.description }}\r\n              </td>\r\n              <td>\r\n                {{ brandNames[Products.brandId] || \"N/A\" }}\r\n              </td>\r\n              <td>\r\n                {{ categoryNames[Products.categoryId] || \"N/A\" }}\r\n              </td>\r\n              <td>\r\n                {{ Products.barCode }}\r\n              </td>\r\n              <td>\r\n                <button class=\"btn btn-primary\" v-on:click=\"editar(Products)\">\r\n                  Editar\r\n                </button>\r\n              </td>\r\n              <td>\r\n                <button\r\n                  class=\"btn btn-danger\"\r\n                  v-on:click=\"excluir(Products.id)\"\r\n                >\r\n                  Excluir\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SideBarDash from \"@/components/SideBarDash.vue\";\r\nimport axios from \"axios\";\r\n\r\n\r\nexport default {\r\n  name: \"CadastroProduto\",\r\n  components: {\r\n    SideBarDash,\r\n  },\r\n\r\n  props: {\r\n    msg: String,\r\n  },\r\n\r\n  data: () => {\r\n    return {\r\n      Product: [],\r\n      Products: undefined,\r\n      Product: \"\",\r\n      //Recente Dropdowns\r\n      Category: [],\r\n      Brand: [],\r\n      brandNames: {},\r\n      categoryNames: {},\r\n      selectedCategory: null,\r\n      selectedBrand: null,\r\n      imageFile: null,\r\n      /////\r\n    };\r\n  },\r\n  //Recente Dropdowns\r\n\r\n  ////\r\n  methods: {\r\n    \r\n\r\n    // DROPDOWN SCRIPT\r\n    async fetchCategoriesAndBrands() {\r\n      try {\r\n        const categoryResponse = await axios.get(\r\n          \"https://localhost:3000/Category\"\r\n        );\r\n        const brandResponse = await axios.get(\"https://localhost:3000/Brand\");\r\n\r\n        this.Category = categoryResponse.data;\r\n        this.Brand = brandResponse.data;\r\n      } catch (error) {\r\n        console.error(\"Error fetching data:\", error);\r\n      }\r\n    },\r\n    //Lista os Produtos na tela\r\n    lista() {\r\n      axios.get(\"https://localhost:3000/Product\").then((res) => {\r\n        this.Product = res.data;\r\n        // console.log(res);\r\n      });\r\n    },\r\n    salvar() {\r\n      \r\n      if (this.Products) {\r\n        this.alterar();\r\n        return;\r\n      }\r\n\r\n      // Obtenha os valores selecionados dos menus dropdown\r\n      const categoryId = this.selectedCategory;\r\n      const brandId = this.selectedBrand;\r\n\r\n      // console.log(\"categoryId:\", categoryId); // Verifica categoryId no console\r\n      // console.log(\"brandId:\", brandId); // Verifica brandId no console\r\n      const productData = {\r\n          sku: document.getElementById(\"sku\").value,\r\n          name: document.getElementById(\"name\").value,\r\n          brandId: brandId, // Envie o CategoryId\r\n          categoryId: categoryId, // Envie o BrandId\r\n          description: document.getElementById(\"description\").value,\r\n          price: document.getElementById(\"price\").value,\r\n          barCode: document.getElementById(\"barCode\").value,\r\n          amount: document.getElementById(\"amount\").value,\r\n      }\r\n      // Adicione a imagem ao objeto FormData\r\n  const imageFile = this.$refs.fileInput.files[0];\r\n  productData.append(\"image\", imageFile);\r\n\r\n      axios\r\n    .post(\"https://localhost:3000/Product\", productData)\r\n    .then((response) => {\r\n      const productId = response.data.id; // Obtém o ID do produto\r\n      console.log(response.data.id);\r\n\r\n      // Renomeie a imagem com base no ID do produto\r\n      const newImageName = `${productId}_${imageFile.name}`;\r\n\r\n// Salve a imagem localmente na pasta \"public/imgproducts\" do projeto Vue.js com o novo nome\r\nconst imageUrl = `imgproducts/${newImageName}`;\r\n\r\n// Copie o arquivo de origem (imageFile) para o destino (imageUrl)\r\nconst destinationPath = `public/${imageUrl}`;\r\n\r\n// Use a API de File do HTML5 para copiar a imagem localmente\r\nconst reader = new FileReader();\r\nreader.onload = () => {\r\n  const imageData = reader.result.split(',')[1]; // Obtém os dados binários da imagem\r\n  const blob = new Blob([new Uint8Array(atob(imageData).split('').map(char => char.charCodeAt(0)))], { type: 'image/jpeg' }); // Cria um blob a partir dos dados binários\r\n\r\n  const file = new File([blob], newImageName, { type: 'image/jpeg' }); // Cria um novo arquivo a partir do blob\r\n\r\n  // Realize a cópia da imagem localmente (pode usar a biblioteca FileSaver.js ou outra opção adequada)\r\n  // Certifique-se de lidar com erros apropriadamente\r\n};\r\n\r\nreader.readAsDataURL(imageFile);\r\nthis.lista();\r\n    this.lista();\r\n})\r\n.catch((error) => {\r\nconsole.error(\"Erro ao fazer upload dos dados do produto:\", error);\r\n});\r\n    },\r\n    //Auto preencher dados para a edição Products\r\n    editar(Products) {\r\n      document.getElementById(\"sku\").value = Products.sku;\r\n      document.getElementById(\"name\").value = Products.name;\r\n      document.getElementById(\"brandId\").value = Products.brandId; // Defina o valor do campo de texto da marca (se necessário)\r\n      document.getElementById(\"categoryId\").value = Products.categoryId; // Defina o valor do campo de texto da categoria (se necessário)\r\n      document.getElementById(\"description\").value = Products.description;\r\n      document.getElementById(\"price\").value = Products.price;\r\n      document.getElementById(\"barCode\").value = Products.barCode;\r\n      document.getElementById(\"amount\").value = Products.amount;\r\n\r\n      // Defina os valores dos menus dropdown (categorias e marcas)\r\n      this.selectedCategory = Products.categoryId; // Defina o ID da categoria selecionada\r\n      this.selectedBrand = Products.brandId; // Defina o ID da marca selecionada\r\n\r\n      this.Products = Products;\r\n    },\r\n    //Atualizar dados que ja estao gravados\r\n    alterar() {\r\n      this.Products.sku = document.getElementById(\"sku\").value;\r\n      this.Products.name = document.getElementById(\"name\").value;\r\n      this.Products.brandId = document.getElementById(\"brandId\").value;\r\n      this.Products.categoryId = document.getElementById(\"categoryId\").value;\r\n      this.Products.description = document.getElementById(\"description\").value;\r\n      this.Products.price = document.getElementById(\"price\").value;\r\n      this.Products.barCode = document.getElementById(\"barCode\").value;\r\n      this.Products.amount = document.getElementById(\"amount\").value;\r\n\r\n      axios\r\n        .put(\r\n          `https://localhost:3000/Product/${this.Products.id}`,\r\n          this.Products\r\n        )\r\n        .then(() => {\r\n          this.lista();\r\n          this.Products = undefined;\r\n          document.getElementById(\"sku\").value = \"\";\r\n          document.getElementById(\"name\").value = \"\";\r\n          document.getElementById(\"brandId\").value = \"\";\r\n          document.getElementById(\"categoryId\").value = \"\";\r\n          document.getElementById(\"description\").value = \"\";\r\n          document.getElementById(\"price\").value = \"\";\r\n          document.getElementById(\"barcode\").value = \"\";\r\n          document.getElementById(\"amount\").value = \"\";\r\n        });\r\n    },\r\n    //Metodo EXCLUIR\r\n    excluir(id) {\r\n      if (confirm(\"🚫Deseja realmente excluir o Produto?🚫\")) {\r\n        axios.delete(`https://localhost:3000/Product/${id}`).then(() => {\r\n          this.lista();\r\n        });\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchCategoriesAndBrands();\r\n    this.lista();\r\n\r\n    // TROCA DE ID FOR NAME\r\n    // Fazer uma chamada para buscar todos os produtos\r\n    axios\r\n      .get(\"https://localhost:3000/Product\")\r\n      .then((response) => {\r\n        this.products = response.data;\r\n      })\r\n      .catch((error) => {\r\n        console.error(\"Erro