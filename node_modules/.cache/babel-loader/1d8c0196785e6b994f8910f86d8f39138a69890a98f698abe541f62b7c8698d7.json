ategory\r\n    editar(Categorys) {\r\n      document.getElementById(\"name\").value = Categorys.name\r\n      this.Categorys = Categorys\r\n    },\r\n    //Atualizar dados que ja estao gravados\r\n    alterar() {\r\n      this.Categorys.name = document.getElementById(\"name\").value \r\n      \r\n      axios.put(`https://localhost:3000/Category/${this.Categorys.id}`, this.Categorys).then(()=>{\r\n        this.lista()\r\n        this.Categorys = undefined\r\n        document.getElementById(\"name\").value = \"\"\r\n      })\r\n    },\r\n    //Metodo EXCLUIR \r\n    excluir(id) {\r\n      if(confirm(\"ðŸš«Deseja realmente excluir a Categoria?ðŸš«\")){\r\n      axios.delete(`https://localhost:3000/Category/${id}`).then(()=>{\r\n        this.lista()\r\n        })\r\n      }\r\n    }\r\n},\r\n  created(){\r\n    this.lista()\r\n  }\r\n\r\n  }\r\n</script>"],"mappings":"AA8CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACPC,GAAG,EAAEC;EACP,CAAC;EACDC,IAAI,EAACA,CAAA,KAAM;IACX,OAAO;MACLC,KAAK,EAAC,EAAE;MACRC,MAAM,EAAEC,SAAS;MACjBF,KAAK,EAAE;IAEP;EACF,CAAC;EACDG,OAAO,EAAC;IACNC,KAAIA,CAAA,EAAK;MACPV,KAAK,CAACW,GAAG,CAAC,8BAA8B,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAG;QAErD,IAAI,CAACC,QAAO,GAAID,GAAG,CAACR,IAAG;QACvBU,OAAO,CAACC,GAAG,CAACH,GAAG;MAChB,CAAC;IACH,CAAC;IACDI,MAAKA,CAAA,EAAK;MACR,IAAG,IAAI,CAACC,SAAS,EAAC;QAChB,IAAI,CAACC,OAAO,CAAC;QACb;MACF;MACAnB,KAAK,CAACoB,IAAI,CAAC,iCAAiC,EAC5C;QACEnB,IAAI,EAACoB,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC;MACvC,CAAC,CAAC,CAACX,IAAI,CAAC,MAAI;QACV,IAAI,CAACF,KAAK,CAAC;QACXK,OAAO,CAACC,GAAG,CAACH,GAAG;MACjB,CAAC;IACH,CAAC;IACL;IACIW,MAAMA,CAACN,SAAS,EAAE;MAChBG,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAI,GAAIL,SAAS,CAACjB,IAAG;MACrD,IAAI,CAACiB,SAAQ,GAAIA,SAAQ;IAC3B,CAAC;IACD;IACAC,OAAOA,CAAA,EAAG;MACR,IAAI,CAACD,SAAS,CAACjB,IAAG,GAAIoB,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,KAAI;MAE1DvB,KAAK,CAACyB,GAAG,CAAE,mCAAkC,IAAI,CAACP,SAAS,CAACQ,EAAG,EAAC,EAAE,IAAI,CAACR,SAAS,CAAC,CAACN,IAAI,CAAC,MAAI;QACzF,IAAI,CAACF,KAAK,CAA